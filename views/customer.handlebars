            <div class="container-fluid" id="chart-container">
                                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            Customers
                            <button class = "btn btn-primary" id="addCompany" data-toggle="modal" data-target="#createModal"><i class="fa fa-fw fa-building"></i> Add Company</button>
                        </h1>                        
                    </div>
                </div>
                <!-- /.row -->

                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title"><i class="fa fa-bar-chart-o fa-fw"></i> Customers</h3>
                                
                            </div>
                            <div class="panel-body">
                                    <table class="table table-striped">
                                        <tr>
                                            <th>Company Name</th>
                                            <th class="text-center">Account #</th>
                                            <th class="">Property Name</th>
                                            <th class="text-center">Tracking ID</th>
                                            <th class="">View Name</th>
                                            <th class="text-center">Google View ID</th>
                                            <th class="text-center">View Actions</th>                                           
                                        </tr>

                                            {{#each companyInfo}}
                                            
                                        <tr>
                                            <td width="20%"><input type="text" class="form-control" id="companyEditName{{id}}" value="{{company_name}}" disabled></td>
                                            <td width="12%"><input type="text" class="form-control text-center" id="accountEdit{{id}}" value="{{account_number}}" disabled></td>
                                            {{#each Properties}}{{#each Views}}
                                            <td width="14%"><input type="text" class="form-control" id="propertyEditName{{id}}" value="{{../property_name}}" disabled></td>
                                            <td width="14%"><input type="text" class="form-control text-center" id="trackingEditId{{id}}" value="{{../tracking_id}}" disabled></td>
                                            <td width="14%"><input type="text" class="form-control" id="viewEditName{{id}}" value="{{view_name}}" disabled></td>
                                            <td width="12%"><input type="text" class="form-control text-center" id="viewEditId{{id}}" value="{{ga_view_id}}" disabled></td>
                                            <td width="14%" class="text-center">                                                
                                                <button class="edit btn btn-warning" data-value="{{id}}"><span class="fa fa-pencil" title='Update View'></span></button>
                                                <button class="save btn btn-success" data-value="{{id}}"><span class="fa fa-floppy-o" title='Save View'></span></button>
                                                <button class="delete btn btn-danger" data-value="{{../../id}}" data-toggle="modal" data-target="#deleteCompanyModal"><span class="fa fa-trash-o" title='Delete Company'></span></button>
                                            </td>
                                            {{/each}}{{/each}}
                                        </tr>                                   
                                            {{/each}} 
